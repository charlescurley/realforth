      <text:p text:style-name="Table_20_Contents">Scroll Lock</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="hatariKeySubstitution.B2" office:value-type="string">
      <text:p text:style-name="Table_20_Contents">will emulate the ST&apos;s UNDO key</text:p>
     </table:table-cell>

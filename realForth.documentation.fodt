      <text:list-item>
       <text:p text:style-name="P139"><text:alphabetical-index-mark-start text:id="IMark94463447495928"/><text:alphabetical-index-mark-start text:id="IMark94463443163416"/><text:span text:style-name="Teletype">&apos;</text:span><text:alphabetical-index-mark-start text:id="IMark94463437228344"/><text:span text:style-name="Teletype">CR</text:span><text:alphabetical-index-mark-end text:id="IMark94463437228344"/><text:alphabetical-index-mark-end text:id="IMark94463443163416"/><text:alphabetical-index-mark-end text:id="IMark94463447495928"/></text:p>
      </text:list-item>
